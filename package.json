{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "concurrently \"npm:watch:css\" \"npm:dev:web\"",
    "build": "remix build",
    "dev:web": "remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "lint": "eslint \"**/*.{tsx,ts,jsx,js}\" --no-error-on-unmatched-pattern",
    "cy:open": "cypress open",
    "cypress": "start-server-and-test dev http://localhost:3000 cy:open",
    "cy:run": "cypress run",
    "cypress:ci": "start-server-and-test dev http://localhost:3000 cy:run",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@testing-library/cypress": "^8.0.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "cypress": "^9.1.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-tailwindcss": "^1.17.2",
    "jest": "^27.3.1",
    "postcss": "^8.4.3",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.0.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
